{
    "name": "hampni/backupdb",
    "description": "Makes backup of database",
    "type": "library",
    "version": "1.0.7",
    "license": "MIT",
    "authors": [
        {
            "name": "hampni",
            "email": "magnus9044@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hampni\\Backupdb\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hampni\\Backupdb\\Providers\\BackupDbProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag={Hampni\\Backupdb\\Providers\\BackupDbProvider}--force"
        ]
    }
}
